n <- 10  # Set number of terms here
a <- 0
b <- 1
cat("Fibonacci sequence: ")
for (i in 1:n) {
  cat(a, " ")
  temp <- a + b
  a <- b
  b <- temp
}
cat("\n")
year <- 2024  # Set year here
if ((year %% 4 == 0 && year %% 100 != 0) || (year %% 400 == 0)) {
  print(paste(year, "is a leap year"))
} else {
  print(paste(year, "is not a leap year"))
}
num <- 7      # Set the number here
start <- 1    # Set start of interval
end <- 10     # Set end of interval
cat("Multiplication Table for", num, "from", start, "to", end, ":\n")
for (i in start:end) {
  cat(num, "x", i, "=", num * i, "\n")
}
nums <- c(2, 5, 7, 10)  # Set numbers here
sum_nums <- sum(nums)
cat("Sum of given numbers:", sum_nums, "\n")
start <- 100   # Set start of interval
end <- 999     # Set end of interval
cat("Armstrong numbers between", start, "and", end, "are:\n")
for (num in start:end) {
  n <- nchar(as.character(num))
  sum <- 0
  temp <- num
  while (temp > 0) {
    digit <- temp %% 10
    sum <- sum + (digit^n)
    temp <- temp %/% 10
  }
  if (sum == num) {
    cat(num, " ")
  }
}
cat("\n")
n <- 50  # Set upper limit here
cat("Prime numbers up to", n, "are:\n")
for (num in 2:n) {
  is_prime <- TRUE
  for (i in 2:floor(sqrt(num))) {
    if (num %% i == 0) {
      is_prime <- FALSE
      break
    }
  }
  if (is_prime) {
    cat(num, " ")
  }
}
a <- 1  # Set coefficient a
b <- -3 # Set coefficient b
c <- 2  # Set coefficient c
d <- b^2 - 4*a*c
if (d > 0) {
  root1 <- (-b + sqrt(d)) / (2*a)
  root2 <- (-b - sqrt(d)) / (2*a)
  cat("Roots are real and different:\n")
  cat("Root 1 =", root1, "\n")
  cat("Root 2 =", root2, "\n")
} else if (d == 0) {
  root <- -b / (2*a)
  cat("Roots are real and same:\n")
  cat("Root =", root, "\n")
} else {
  realPart <- -b / (2*a)
  imagPart <- sqrt(-d) / (2*a)
  cat("Roots are complex and different:\n")
  cat("Root 1 =", realPart, "+", imagPart, "i\n")
  cat("Root 2 =", realPart, "-", imagPart, "i\n")
}
num <- 5  # Set the number here
fact <- 1
if (num < 0) {
  cat("Factorial does not exist for negative numbers\n")
} else {
  for (i in 1:num) {
    fact <- fact * i
  }
  cat("Factorial of", num, "is", fact, "\n")
}
num <- 28  # Set the number here
cat("Factors of", num, "are:\n")
for (i in 1:num) {
  if (num %% i == 0) {
    cat(i, " ")
  }
}
cat("\n")
num <- 25  # Set the decimal number here
binary <- ""
n <- num
if (n == 0) {
  binary <- "0"
} else {
  while (n > 0) {
    binary <- paste0(n %% 2, binary)
    n <- n %/% 2
  }
}
cat("Binary representation of", num, "is", binary, "\n")
